{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "one-rep-max",
  "title": "one-rep-max",
  "description": "An extension to easily calculate your one rep max in weight lifting by providing your current weight and repetitons.",
  "icon": "command-icon.png",
  "author": "marianbreitmeyer",
  "license": "MIT",
  "arguments": [
    {
      "type": "textfield",
      "required": "true"
    }
  ],
  "commands": [
    {
      "name": "rep-max",
      "title": "rep max",
      "subtitle": "calculate your one rep max",
      "description": "",
      "mode": "view",
      "arguments": [
        {
          "name": "weight",
          "placeholder": "weight",
          "type": "text",
          "required": true
        },
        {
          "name": "reps",
          "placeholder": "repetitions",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.2",
    "@raycast/utils": "^1.13.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
